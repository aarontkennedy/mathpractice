{
  "name": "mathpractice",
  "version": "1.0.0",
  "description": "Math Practice Web App",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aarontkennedy/mathpractice.git"
  },
  "keywords": [
    "Math"
  ],
  "author": "Aaron Kennedy",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aarontkennedy/mathpractice/issues"
  },
  "homepage": "https://github.com/aarontkennedy/mathpractice#readme",
  "devDependencies": {
    "concurrently": "^3.5.0",
    "nodemon": "^1.11.0",
    "if-env": "^1.0.4"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "if-env": "^1.0.4",
    "mysql": "^2.15.0"
  }
}
